[filter "lfs"]
	clean = git lfs clean %f
	smudge = git lfs smudge %f
	required = true
[user]
	name = CHANN
	email = chann@chann.kr
[core]
	editor = vim
[alias]
	cm = commit -m
	co = checkout
	lg = log --decorate --oneline --graph --all
    po = pull origin
    poo = push origin
	f = fetch
	ds = diff --staged
	di = diff
